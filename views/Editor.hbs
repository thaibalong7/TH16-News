<div class="list_cata">
    <div>
        <div>
            <div>
                <div class="col-md-12">
                    <div class="row">
                        <div>
                            <div class="list-cata">
                                {{#each category}}
                                <h1>Danh sách bài viết chuyên mục {{name}}</h1>
                                {{/each}}
                                <div id="post-list-category">
                                    {{#each news}}
                                    <div class="col-md-12">
                                        <div class="post post-row">
                                            <a class="post-img" href="#"><img src="./img/{{img}}"></a>
                                            <div class="post-body">
                                                <div class="post-meta">
                                                    <a class="post-category" href="#">{{type}}</a>
                                                    <a class="post-category cat-4" href="#">{{category}}</a>
                                                    <span class="post-date">{{publicAt}}</span>
                                                    <a class="post-category cat-3" href="#">Duyệt</a>
                                                     <a class="post-category cat-2" href="#">Từ chối</a>
                                                </div>
                                                <h3 class="post-title"><a>{{name}}</a></h3>
                                                <p class="post-abstract">{{abstract}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>